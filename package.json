{
  "name": "django-formset",
  "license": "MIT",
  "private": true,
  "scripts": {
    "esbuild": "node client/esbuild.cjs",
    "esbuild.monolith": "node client/esbuild.cjs --monolith",
    "rollup": "rollup --config client/rollup.config.js",
    "minify": "terser --compress --mangle --output formset/static/formset/js/django-formset.min.js -- formset/static/formset/js/django-formset.js",
    "tag-attributes": "peggy -o client/build/tag-attributes.ts --allowed-start-rules Ternary,OperabilityExpression,InduceExpression --extra-options-file assets/pegjsconf.json --plugin ./node_modules/ts-pegjs/dist/tspegjs assets/tag-attributes.pegjs",
    "function-code": "peggy -o client/build/function-code.ts --allowed-start-rules FunctionCode --extra-options-file assets/pegjsconf.json --plugin ./node_modules/ts-pegjs/dist/tspegjs assets/function-code.pegjs",
    "compilescss": "sass --load-path=. assets/scss:formset/static/formset/css",
    "tailwindcss": "tailwindcss --input assets/tailwind-styles.css --config assets/tailwind.config.js --output formset/static/formset/css/tailwind.css",
    "docscss": "postcss docs/source/_templates/css/nested-styles.text --config assets/postcss.config.js --output docs/build/json/_static/styles/nested-styles.css",
    "testclient": "jest"
  },
  "dependencies": {
    "flag-icons": "^6.15.0"
  },
